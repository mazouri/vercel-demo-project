{
    "version": 2,
    "builds": [
        {
            "src": "frontend",
            "use": "@vercel/node",
            "config": {
                "runtime": "nodejs14.x",
                "buildCommand": "npm run build",
                "outputDirectory": "frontend/build"
            }
        }
        {
            "src": "backend/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/public/$1"
        },
        {
            "src": "/example-api",
            "dest": "/backend/main.py"
        }
    ]
}